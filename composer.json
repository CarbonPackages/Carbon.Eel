{
    "name": "carbon/eel",
    "type": "neos-carbon",
    "description": "Some nice Eel helper for Neos.io",
    "license": "MIT",
    "keywords": ["neos", "fusion", "helper", "eel", "carbon"],
    "require": {
        "php": "^8.1",
        "neos/flow": "^7.0 || ^8.0 || ^9.0",
        "behat/transliterator": "^1.2",
        "gehrisandro/tailwind-merge-php": "^1.0",
        "matthiasmullie/minify": "^1.3"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Eel\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Carbon.Eel",
        "neos": {
            "package-key": "Carbon.Eel"
        }
    }
}
